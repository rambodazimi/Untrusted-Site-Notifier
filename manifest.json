{
  "manifest_version": 3,
  "name": "Untrusted Site Notifier",
  "version": "1.0",
  "description": "Notifies the user when visiting an untrusted website",
  "permissions": [
    "webNavigation",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  }
}
